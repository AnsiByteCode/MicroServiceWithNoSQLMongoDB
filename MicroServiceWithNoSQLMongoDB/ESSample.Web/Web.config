<configuration>
  <appSettings>
    <add key="webpages:Version" value="3.0.0.0"/>
    <add key="webpages:Enabled" value="false"/>
    <add key="ClientValidationEnabled" value="true"/>
    <add key="UnobtrusiveJavaScriptEnabled" value="true"/>

    <add key="ServiceAddress" value="rabbitmq://localhost/test/createuser" />
    <add key="ServiceAddressInMemory" value="loopback://localhost/createuser" />
    <add key="RabbitMqUri" value="rabbitmq://localhost/test" />
    <add key="RabbitMqUser" value="guest" />
    <add key="RabbitMqPassword" value="guest" />
    <add key="Endpoint" value="createuser" />
    <add key="UseInMemoryBus" value="false" />
    <add key="SnapshotIntervallInSeconds" value="120" />
    <add key="ConnectionString" value="mongodb://192.168.1.216:27017" />
    <add key="DatabaseName" value="EventDB" />

    <!--Cart Service-->
    <add key="CartServicePort" value="5267" />
    <add key="CartServiceDisplayName" value="Cart Service" />
    <add key="CartServiceName" value="CartService" />
    <add key="CartCollectionName" value="Cart" />

    <!--Customer Service-->
    <add key="CustomerServicePort" value="5265" />
    <add key="CustomerServiceDisplayName" value="Customer Service" />
    <add key="CustomerServiceName" value="CustomerService" />
    <add key="CustomerCollectionName" value="Customers" />

    <!--Order Service-->
    <add key="OrderServicePort" value="5268" />
    <add key="OrderServiceDisplayName" value="Order Service" />
    <add key="OrderServiceName" value="OrderService" />
    <add key="OrderCollectionName" value="Orders" />
    <add key="OrderDetailCollectionName" value="OrderDetails" />

    <!--Product Service-->
    <add key="ProductServicePort" value="5266" />
    <add key="ProductServiceDisplayName" value="Product Service" />
    <add key="ProductServiceName" value="ProductService" />
    <add key="ProductCollectionName" value="Products" />
  </appSettings>
  <!--
    For a description of web.config changes see http://go.microsoft.com/fwlink/?LinkId=235367.

    The following attributes can be set on the <httpRuntime> tag.
      <system.Web>
        <httpRuntime targetFramework="4.6.2" />
      </system.Web>
  -->
  <system.web>
    <compilation debug="true" targetFramework="4.6.2"/>
    <httpRuntime targetFramework="4.5.2"/>
    <httpModules>
      <add name="ApplicationInsightsWebTracking" type="Microsoft.ApplicationInsights.Web.ApplicationInsightsHttpModule, Microsoft.AI.Web"/>
    </httpModules>
    <authentication mode="Forms">
      <forms loginUrl="~/Account/Login" protection="All" timeout="30" name=".ASPXAUTH" path="/" requireSSL="false" slidingExpiration="true" defaultUrl="~/Home/Index" cookieless="UseDeviceProfile" enableCrossAppRedirects="false"/>
    </authentication>
    <customErrors mode="On" defaultRedirect="~/Account/Error">
      
    </customErrors>
  </system.web>
  <runtime>
    <assemblyBinding xmlns="urn:schemas-microsoft-com:asm.v1">
